generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model OrderData {
  id        Int
  orderId   String
  orderTime String
  productId String
  variantId String
  price     Float
  quantity  Int
  updatedAt DateTime @updatedAt
  createdAt DateTime @default(nonw())
}

model ProductSales {
  id              Int      @id @default(autoincrement())
  productId       String   @unique
  title           String
  salesIn30Days   Int
  salesIn45Days   Int
  salesIn90Days   Int
  price           Float
  revenueIn30Days Float
  revenueIn45Days Float
  revenueIn90Days Float
  updatedAt       DateTime @updatedAt
  createdAt       DateTime @default(now())
}

model VariantSalesInfo {
  id                Int      @id @default(autoincrement())
  productId         String
  variantId         String   @unique
  title             String
  price             Float
  options           Json
  inventoryQuantity Int
  salesIn30Days     Int
  salesIn45Days     Int
  salesIn90Days     Int
  revenueIn30Days   Float
  revenueIn45Days   Float
  revenueIn90Days   Float
  updatedAt         DateTime @updatedAt
  createdAt         DateTime @default(now())
}
